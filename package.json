{
  "name": "@aics/volume-viewer",
  "version": "3.11.1",
  "description": "volume renderer for multichannel 8-bit intensity data stored as 3D arrays",
  "main": "es/index.js",
  "type": "module",
  "module": "es/index.js",
  "types": "es/types/index.d.ts",
  "files": [
    "es",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build-docs": "node node_modules/documentation/bin/documentation.js build src/Histogram.ts src/View3d.ts src/Volume.ts src/VolumeMaker.ts src/VolumeLoader.ts -f html -o docs --shallow",
    "build": "npm run transpileES && npm run build-types",
    "build-types": "tsc -p tsconfig.types.json",
    "build-demo": "webpack --config webpack.demo.js",
    "clean": "rimraf es/",
    "format": "prettier --write src/**/*.js",
    "gh-build": "webpack --config webpack.dev.js",
    "dev": "webpack serve --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js",
    "react-example": "webpack serve --config react-example/webpack.react.js",
    "lint": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore --ext .jsx --ext .js --ext .ts ./src",
    "test": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" mocha --require ts-node/register --es-module-specifier-resolution=node src/**/test/*.[jt]s",
    "transpileES": "babel src --out-dir es --extensions .js,.ts --ignore **/*.test.js",
    "typeCheck": "tsc -p tsconfig.json --noEmit"
  },
  "author": "Daniel Toloudis",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "geotiff": "^2.0.5",
    "serialize-error": "^11.0.3",
    "three": "^0.162.0",
    "tweakpane": "^3.1.9",
    "zarrita": "^0.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@tweakpane/core": "^1.1.9",
    "@types/chai": "^4.3.12",
    "@types/mocha": "^9.1.1",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "acorn": "^8.7.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "chai": "^5.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "documentation": "^14.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "lil-gui": "^0.19.2",
    "lint-staged": "^13.2.1",
    "mocha": "^10.3.0",
    "prettier": "^2.3.2",
    "raw-loader": "^4.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
