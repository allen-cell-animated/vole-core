{
  "name": "@aics/volume-viewer",
  "version": "2.1.1",
  "description": "volume renderer for multichannel 8-bit intensity data stored as 3D arrays",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build-docs": "node node_modules/documentation/bin/documentation.js build src/Histogram.js src/View3d.js src/Volume.js src/VolumeMaker.js src/volumeLoader.js -f html -o docs --shallow",
    "build": "npm run transpileES",
    "build-demo": "webpack --config webpack.demo.js",
    "clean": "rimraf es/",
    "format": "prettier --write src/**/*.js",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "react-example": "webpack-dev-server --config react-example/webpack.react.js",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --ext .jsx --ext .js ./",
    "test": "mocha --require @babel/register src/**/test/*.js",
    "transpileES": "babel src --out-dir es --extensions .js --ignore src/**/*.test.js"
  },
  "author": "Daniel Toloudis",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "three": "^0.126.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.11.5",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "chai": "^4.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "documentation": "^13.0.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "mocha": "^8.1.3",
    "nyc": "^14.1.1",
    "prettier": "^1.16.1",
    "raw-loader": "^0.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^2.7.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.0",
    "worker-loader": "^1.0.0"
  }
}
