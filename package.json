{
  "name": "@aics/volume-viewer",
  "version": "2.1.1",
  "description": "volume renderer for multichannel 8-bit intensity data stored as 3D arrays",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build-docs": "node node_modules/documentation/bin/documentation.js build src/Histogram.js src/View3d.js src/Volume.js src/VolumeMaker.js src/volumeLoader.js -f html -o docs --shallow",
    "build": "npm run transpileES",
    "build-demo": "webpack --config webpack.demo.js",
    "clean": "rimraf es/",
    "format": "prettier --write src/**/*.js",
    "dev": "webpack serve --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js",
    "react-example": "webpack serve --config react-example/webpack.react.js",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --ext .jsx --ext .js ./",
    "test": "mocha --require @babel/register src/**/test/*.js",
    "transpileES": "babel src --out-dir es --extensions .js --ignore src/**/*.test.js"
  },
  "author": "Daniel Toloudis",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "three": "^0.130.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "chai": "^4.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "documentation": "^13.2.5",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.0",
    "mocha": "^8.1.3",
    "prettier": "^2.3.2",
    "raw-loader": "^4.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
